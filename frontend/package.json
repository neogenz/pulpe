{
  "name": "pulpe-frontend",
  "version": "0.9.0",
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "ng": "ng",
    "prestart": "pnpm run generate:config && pnpm run generate:build-info",
    "start": "ng serve --open",
    "prestart:ci": "pnpm run generate:config && pnpm run generate:build-info",
    "start:ci": "ng serve",
    "predev": "pnpm run generate:config && pnpm run generate:build-info",
    "dev": "ng serve --host 0.0.0.0",
    "prebuild": "pnpm run generate:config && pnpm run generate:build-info",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "generate:build-info": "node scripts/generate-build-info.js",
    "generate:config": "tsx scripts/generate-config.ts",
    "pretest": "pnpm run generate:build-info",
    "test": "vitest run",
    "test:strict": "pnpm run typecheck:spec && vitest run",
    "test:watch": "pnpm run generate:build-info && vitest",
    "typecheck": "pnpm exec tsc --noEmit",
    "pretypecheck:spec": "pnpm run generate:build-info",
    "typecheck:spec": "pnpm exec tsc --noEmit --project projects/webapp/tsconfig.spec.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:4200",
    "format": "prettier --write \"./projects/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --list-different \"./projects/**/*.{ts,html,scss,json}\"",
    "lint": "ng lint",
    "upload:sourcemaps": "node scripts/upload-sourcemaps.js",
    "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template treemap --metadata dist/webapp/stats.json --filename dist/webapp/analyse/index.html && http-server -o -c-1 ./dist/webapp/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing none --named-chunks && source-map-explorer dist/webapp/browser/*.js --html dist/webapp/sme/index.html && http-server -o -c-1 ./dist/webapp/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/webapp/src/main.ts --ts-config projects/webapp/tsconfig.app.json --image ./deps/webapp/_all.jpg",
    "analyze:deps:ui": "madge --extensions ts --ts-config projects/webapp/tsconfig.app.json --image ./deps/webapp/ui.png projects/webapp/src/app/ui/",
    "deps:circular": "madge --circular projects/webapp/src/app --extensions ts --ts-config projects/webapp/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "21.0.5",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/material": "21.0.5",
    "@angular/material-date-fns-adapter": "21.0.5",
    "@angular/platform-browser": "^21.0.6",
    "@angular/router": "^21.0.6",
    "pulpe-shared": "workspace:*",
    "@supabase/supabase-js": "^2.49.10",
    "@tailwindcss/postcss": "^4.1.17",
    "date-fns": "^4.1.0",
    "driver.js": "^1.4.0",
    "lottie-web": "^5.13.0",
    "ngx-lottie": "^20.0.0",
    "ngx-turnstile": "^0.2.5",
    "postcss": "^8.5.4",
    "posthog-js": "^1.260.2",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.57.0",
    "@posthog/cli": "^0.4.7",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "angular-eslint": "21.1.0",
    "dotenv": "^17.2.1",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "source-map-explorer": "^2.5.3",
    "tsx": "^4.20.4",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  }
}