{
  "name": "pulpe-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:vitest": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:vitest:run": "vitest run",
    "format:test": "prettier --list-different \"./projects/**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/*.{ts,html,scss,json}\"",
    "lint": "ng lint",
    "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template treemap --metadata dist/webapp/stats.json --filename dist/webapp/analyse/index.html && http-server -o -c-1 ./dist/webapp/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing none --named-chunks && source-map-explorer dist/webapp/browser/*.js --html dist/webapp/sme/index.html && http-server -o -c-1 ./dist/webapp/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/webapp/src/main.ts --ts-config projects/webapp/tsconfig.app.json --image ./deps/webapp/_all.jpg",
    "analyze:deps:ui": "madge --extensions ts --ts-config projects/webapp/tsconfig.app.json --image ./deps/webapp/ui.png projects/webapp/src/app/ui/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "20.0.1",
    "@angular/material-date-fns-adapter": "20.0.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@pulpe/shared": "workspace:*",
    "@supabase/supabase-js": "^2.49.10",
    "@tailwindcss/postcss": "^4.1.8",
    "date-fns": "^4.1.0",
    "postcss": "^8.5.4",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.8",
    "tslib": "^2.3.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.0.0",
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@eslint/js": "^9.28.0",
    "@vitest/ui": "^3.1.4",
    "angular-eslint": "20.0.0",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.33.1",
    "vitest": "^3.2.3"
  }
}
